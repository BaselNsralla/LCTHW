#include <stdio.h>

int main(int argc, char *argv[]){
  int distance = 100;
  float power = 2.345f;
  double super_power = 56789.4543;
  char initial = 'A';
  char first_namr[] = "Zed";
  char last_name[]  = "theman";
  printf("You are %d miles away \n", distance);
  printf("You have %f of power \n", power);
  printf("You have %f awesome super powers \n", super_powers);
  printf("I have an initial %c. \n", initial);
  printf("I have first name %s", first_name);
  printf("I have last name %s", last_name);
  print("MY full name is %s %c %s", first_name, initial, last_name);
  
 }
